extends ../_includes/layout

block title
    title !{__.CookieList.TITLE}

block meta
    meta(property="og:type" content="website")
    meta(property="og:title" content=__.CookieList.META_TITLE)
    meta(property="og:description" content=__.CookieList.META_DESCRIPTION)
    meta(property="og:image" content=__.CookieList.META_IMAGE)

block content
    +loader()
    +generic-page(documents.CookieList)
