extends ../_includes/layout

block title
    title !{__.ComplaintsPolicy.TITLE}

block meta
    meta(property="og:type" content="website")
    meta(property="og:title" content=__.ComplaintsPolicy.META_TITLE)
    meta(property="og:description" content=__.ComplaintsPolicy.META_DESCRIPTION)
    meta(property="og:image" content=__.ComplaintsPolicy.META_IMAGE)

block content
    +loader()
    +generic-page(documents.ComplaintsPolicy)
